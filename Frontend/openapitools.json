{
    "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
    "spaces": 4,
    "generator-cli": {
        "version": "7.0.0",
        "generators": {
            "zapp-be-api": {
                "generatorName": "typescript-fetch",
                "output": "./src/lib/generated/backend-api",
                "glob": "../Backend/build/docs/openapi.json",
                "additionalProperties": {
                    "supportsES6": true,
                    "withInterfaces": true,
                    "enumPropertyNaming": "UPPERCASE"
                }
            }
        }
    }
}
